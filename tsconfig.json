{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node", 
    "outDir": "./dist",
    "rootDir": "./src",
    "sourceMap": true, 
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true, 
    "skipLibCheck": true
  }
}